}
.btn-eon:hover { background: var(--color-dark-teal); color: #fff; }
/* Gradient Circles */
.icon-circle {
  width: 70px;
  height: 70px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  margin-bottom: 1rem;
  box-shadow: 0 8px 20px rgba(0,0,0,0.15);
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.bg-gradient-safety {
  background: linear-gradient(90deg, #0f52ba, #682dc9);
}

.bg-gradient-energy {
  background: linear-gradient(90deg, #0f52ba, #682dc9);
}

.bg-gradient-compact {
  background: linear-gradient(90deg, #0f52ba, #682dc9);
}

/* Hover effect */
.info-card:hover .icon-circle {
  transform: scale(1.1);
  box-shadow: 0 12px 25px rgba(0,0,0,0.25);
}

.hover-up {
  transition: transform 0.3s ease;
}
.hover-up:hover {
  transform: translateY(-8px);
}
/* Subtle background circle */
.icon-circle-subtle {
  width: 70px;
  height: 70px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  background-color: rgba(103, 78, 167, 0.1); /* soft translucent background */
  margin-bottom: 1rem;
  box-shadow: 0 4px 15px rgba(0,0,0,0.1);
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

/* Gradient icons */
.icon-gradient {
  background: linear-gradient(135deg, #674ea7, #44ede8);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

/* Card hover effect */
.info-card {
  background: white;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
  padding: 2rem 1.5rem;
}
.info-card:hover {
  transform: translateY(-10px);
  box-shadow: 0 12px 30px rgba(0,0,0,0.2);
}

/* Scroll Animation using AOS */
[data-aos] {
  opacity: 0;
  transition-property: opacity, transform;
}
[data-aos].aos-animate {
  opacity: 1;
}
/* Ideal Applications Cards */
.app-card {
  background: white;
  transition: transform 0.3s ease, box-shadow 0.3s ease;
