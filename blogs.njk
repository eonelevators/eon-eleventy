 <script>
(function () {
  const lift = document.getElementById('lift');
  const track = document.getElementById('liftTrack');

  let isDragging = false;
  let dragOffset = 0; // pointer offset inside the lift
  let trackRect, trackHeight, liftHeight;

  function docHeights() {
    const doc = document.documentElement;
    const scrollHeight = Math.max(doc.scrollHeight, document.body.scrollHeight) - window.innerHeight;
    return Math.max(0, scrollHeight);
  }

  function calcTrackMetrics() {
    trackRect   = track.getBoundingClientRect();
    liftHeight  = lift.offsetHeight;
    trackHeight = trackRect.height - liftHeight;
  }

  function setLiftY(y) {
    lift.style.transform = `translate(-50%, ${y}px)`;
  }

  function updateFromScroll() {
    if (isDragging) return; // avoid fighting with drag
    const docH = docHeights();
    if (docH === 0) { setLiftY(0); return; }
    const st = window.scrollY || document.documentElement.scrollTop || 0;
    const y = (st / docH) * trackHeight;
    setLiftY(y);
  }

  function scrollFromLiftY(y) {
    const docH = docHeights();
    const pct = (trackHeight === 0) ? 0 : (y / trackHeight);
    const newScroll = pct * docH;
    window.scrollTo(0, newScroll);
  }

  // Start drag (pointer & mouse/touch unified)
  lift.addEventListener('pointerdown', (e) => {
    e.preventDefault();
    isDragging = true;
    calcTrackMetrics();
    dragOffset = e.clientY - lift.getBoundingClientRect().top;
    lift.setPointerCapture(e.pointerId);
    document.body.style.userSelect = 'none';
  });

  // Drag move
  lift.addEventListener('pointermove', (e) => {
    if (!isDragging) return;
    const rawY = e.clientY - trackRect.top - dragOffset;
    const y = Math.max(0, Math.min(trackHeight, rawY));
    setLiftY(y);              // <-- IMPORTANT: move the icon during drag
    scrollFromLiftY(y);       // and scroll the page 1:1
  });

  // End drag
  lift.addEventListener('pointerup', (e) => {
    isDragging = false;
    document.body.style.userSelect = 'auto';
    lift.releasePointerCapture(e.pointerId);
  });
  lift.addEventListener('pointercancel', () => {
    isDragging = false;
    document.body.style.userSelect = 'auto';
  });

  // Sync when user scrolls normally
  window.addEventListener('scroll', updateFromScroll, { passive: true });

  // Recompute & sync on resize
  window.addEventListener('resize', () => { calcTrackMetrics(); updateFromScroll(); });

  // Initial
  calcTrackMetrics();
  updateFromScroll();
})();
  </script>




