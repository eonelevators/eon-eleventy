							  {% endfor %}
							  
							  
							</div>
						  </div>


						 

						  <!-- STEP 7: Contact -->
						  <div class="form-step villaLiftFormSteps" id="your_contact_details_div">
							<h5 class="mb-3 mt-4">Your Contact Details</h5>
