<script>
function initMultiStepForm(formId, sidebarId, progressId, nextBtnId, prevBtnId) {
  const form = document.getElementById(formId);
  if (!form) return;

  let currentStep = 0;
  const steps = form.querySelectorAll('.form-step');
  const sidebar = document.querySelectorAll('#' + sidebarId + ' li');
  const nextBtn = document.getElementById(nextBtnId);
  const prevBtn = document.getElementById(prevBtnId);
  const progress = document.getElementById(progressId);

  function showStep(n) {
    steps.forEach((s, i) => s.classList.toggle('active', i === n));
    sidebar.forEach((li, i) => li.classList.toggle('active', i === n));
    prevBtn.style.visibility = n === 0 ? 'hidden' : 'visible';
    nextBtn.textContent = n === steps.length - 1 ? 'Submit' : 'Next';
	
	if(formId=="villaLiftForm" && n === steps.length - 1){
		nextBtn.type="submit";
	}
	else {
      nextBtn.type = "button";
    }
	
	// üîπ Handle radio steps
    const radios = steps[n].querySelectorAll('input[type="radio"]');
    if (radios.length > 0) {
      // Initially disable
      nextBtn.disabled = true;

      // If any already checked ‚Üí enable immediately
      if (steps[n].querySelector('input[type="radio"]:checked')) {
        nextBtn.disabled = false;
      }

      // Enable when a radio is changed
      radios.forEach(r => {
        r.addEventListener("change", () => {
          nextBtn.disabled = false;
        });
      });
    } else {
      nextBtn.disabled = false; // no radios ‚Üí free to go
    }
	
    if (progress) progress.style.width = ((n + 1) / steps.length * 100) + '%';
  }

  nextBtn.addEventListener('click', () => {
    if (currentStep < steps.length - 1) {
      currentStep++;
      showStep(currentStep);
    } else {
	if(formId=="villaLiftForm"){
		$('#villaLiftForm').bootstrapValidator({
        message: 'This value is not valid',
        feedbackIcons: {
            valid: 'glyphicon glyphicon-ok',
            invalid: 'glyphicon glyphicon-remove',
            validating: 'glyphicon glyphicon-refresh'
        },
        fields: {
			
				name: {
					message: 'Name is not valid',
					validators: {
						notEmpty: {
							message: 'Name is required and cannot be empty'
						},
						stringLength: {
							min: 3,
							max: 30,
							message: 'Name must be more than 3 and less than 30 characters long'
						}
					}
				},
				email: {
					validators: {
						notEmpty: {
							message: 'Email is required and cannot be empty'
						},
						emailAddress: {
							message: 'input is not a valid email address'
						}
					}
				},
				 mobile: {
					validators: {
						notEmpty: {
							message: 'Mobile number is required and cannot be empty '
						},
						stringLength: {
							min: 10,
							max: 10,
							message: 'Mobile number must be 10 characters long '
						},
						regexp: {
							regexp: /^[0-9]+$/,
							message: 'Mobile number can only consist of numbers '
						}
					}
				}
			}
		}).on('success.form.bv', function(e) {
	var form = $('#villaLiftForm');
	form.submit(function(event){
		

		
	event.preventDefault();
	var valuesToSubmit = $(this).serialize();
	$.ajax({
	//url:"http://localhost/eon-elevator-action/contact-action.php",
	url:"http://128.199.22.182/eon-elevator-action/contact-action.php",
	type: "POST",      				// Type of request to be send, called as method
	data:  new FormData(this),	// Data sent to server, a set of key/value pairs representing form fields and values 
	contentType: false,       		// The content type used when sending data to the server. Default is: "application/x-www-form-urlencoded"
	cache: false,					// To unable request pages to be cached
	processData:false,  			// To send DOMDocument or non processed data file it is set to false (i.e. data should not be in the form of string)
	beforeSend:function(){
		nextBtn.innerHTML='Processing &nbsp;<i class="spinner-border spinner-border-sm"></i>';
	}
	}).done(function(data){
		
				 nextBtn.innerHTML="Submit";
				
				
		//alert(data)
		
				if(data.trim()=="yes"){
					Swal.fire({
					
			title: '',
			html: 'Thanks for submitting the details!',
			icon: 'success',
			confirmButtonColor: '#3085d6',
			confirmButtonText: 'OK'
		}).then((result) => {
			location.reload();
		});
				
				}
				else{
					swal({
							title:"Error", 
							text:"An Unexpected Error has occurred, Please try again", 
							type: "error",
							allowOutsideClick: false
						}).then(function () {
							return false;

						});
				}
				
				 
				
			 });
	});
	});



	}
	else{
		form.submit(); // only this form
	}
    }
  });

  prevBtn.addEventListener('click', () => {
    if (currentStep > 0) {
      currentStep--;
      showStep(currentStep);
    }
  });

  form.closest('.modal').addEventListener('shown.bs.modal', () => {
    currentStep = 0;
    showStep(currentStep);
  });

  showStep(currentStep);
}

// ‚úÖ Init both forms
initMultiStepForm('liftForm', 'liftFormSidebar', 'mobileProgress', 'nextBtn', 'prevBtn');
initMultiStepForm('villaLiftForm', 'villaLiftFormSidebar', 'villaMobileProgress', 'villaNextBtn', 'villaPrevBtn');
initMultiStepForm('officeLiftForm', 'officeLiftFormSidebar', 'officeMobileProgress', 'officeNextBtn', 'officePrevBtn');
initMultiStepForm("mallLiftForm", "mallLiftFormSidebar", "mallMobileProgress", "mallNextBtn", "mallPrevBtn");
initMultiStepForm("hospitalLiftForm", "hospitalLiftFormSidebar", "hospitalMobileProgress", "hospitalNextBtn", "hospitalPrevBtn");
initMultiStepForm("warehouseLiftForm", "warehouseLiftFormSidebar", "warehouseMobileProgress", "warehouseNextBtn", "warehousePrevBtn");

</script>




