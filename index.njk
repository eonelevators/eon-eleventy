    // Special handling for carousels
    if (el.classList.contains("swiper-container1")) {
      el.style.overflow = "hidden"; // ✅ lock overflow
    }

    el.classList.remove("fade-out", "slide-out-right", "slide-in-left", "slide-in-right", "fade-in");
    el.classList.add(animation);
  }

  // 🔹 Hide element with animation
  function hideElement(el, animation = "fade-out") {
    el.classList.remove("fade-in", "slide-in-right", "slide-in-left");
    el.classList.add(animation);

    setTimeout(() => { 
      el.style.display = "none"; 
      if (el.classList.contains("swiper-container1")) {
        el.style.overflow = ""; // ✅ reset when hidden
      }
    }, 400);
  }

  // 🔹 Grid → Detail
  readMoreBtns.forEach(btn => {
    btn.addEventListener("click", e => {
      e.preventDefault();
      const target = btn.getAttribute("data-target");

      hideElement(cardsWrapper, "fade-out");

      detailCards.forEach(card => {
        if (card.id === target) showElement(card, "slide-in-right");
        else hideElement(card);
      });
    });
  });

  // 🔹 Detail → Carousel
  exploreBtns.forEach(btn => {
    btn.addEventListener("click", e => {
      e.preventDefault();
      const target = btn.getAttribute("data-target");

      detailCards.forEach(card => hideElement(card, "slide-out-right"));

      carousels.forEach(c => {
        if (c.id === target) showElement(c, "fade-in");
        else hideElement(c);
      });
    });
  });

  // 🔹 Carousel → Detail
  carouselBackBtns.forEach(btn => {
    btn.addEventListener("click", e => {
      e.preventDefault();
      const target = btn.getAttribute("data-back");

      carousels.forEach(c => hideElement(c));

      detailCards.forEach(card => {
        if (card.id === target) showElement(card, "slide-in-left");
        else hideElement(card);
      });
    });
  });

  // 🔹 Detail → Grid
  backBtns.forEach(btn => {
    btn.addEventListener("click", e => {
      e.preventDefault();
      detailCards.forEach(card => hideElement(card, "slide-out-right"));
      carousels.forEach(c => hideElement(c));
      showElement(cardsWrapper, "fade-in", "flex"); // ✅ grid stays flex
    });
  });

  // 🔹 Initial Hide
  detailCards.forEach(card => card.style.display = "none");
  carousels.forEach(c => {
    c.style.display = "none";
    c.style.overflow = ""; // reset
  });
</script>



