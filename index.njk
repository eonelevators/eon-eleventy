$(document).ready(function () {

    $('#villaLiftForm').bootstrapValidator({
        message: 'This value is not valid',
        feedbackIcons: {
            valid: 'glyphicon glyphicon-ok',
            invalid: 'glyphicon glyphicon-remove',
            validating: 'glyphicon glyphicon-refresh'
        },
        fields: {
            name: {
                validators: {
                    notEmpty: {
                        message: 'Name is required and cannot be empty'
                    },
                    stringLength: {
                        min: 3,
                        max: 30,
                        message: 'Name must be 3 to 30 characters'
                    }
                }
            },
            email: {
                validators: {
                    notEmpty: {
                        message: 'Email is required'
                    },
                    emailAddress: {
                        message: 'Invalid email address'
                    }
                }
            },
            mobile: {
                validators: {
                    notEmpty: {
                        message: 'Mobile number is required'
                    },
                    stringLength: {
                        min: 10,
                        max: 10,
                        message: 'Mobile must be 10 digits'
                    },
                    regexp: {
                        regexp: /^[0-9]+$/,
                        message: 'Only numbers allowed'
                    }
                }
            }
        }
    }).on('success.form.bv', function (e) {
		e.preventDefault(); // stop normal submit

		let desktop_mobile = document.body.offsetWidth < 768 ? "mobile" : "desktop";
		document.getElementById("desktop_mobile_villalift_form").value = desktop_mobile;
		document.getElementById("villalift_page_url").value = location.href;
		document.getElementById("villalift_page_name").value = "index.html";

		const form = document.getElementById("villaLiftForm");
		const formData = new FormData(form);

		const payload = {};

		formData.forEach((value, key) => {
		if (key.endsWith("[]")) {
			key = key.replace("[]", "");
			payload[key] = payload[key] || [];
			payload[key].push(value);
		} else {
			payload[key] = value;
		}
		});

		document.getElementById("villalift_submit_btn").innerHTML =
		'Processing <span class="spinner-border spinner-border-sm"></span>';

		fetch("http://voometstudio.com/eon-strapi/api/villa-lift-enquiries", {
		method: "POST",
		headers: {
			"Content-Type": "application/json"
		},
		body: JSON.stringify({ data: payload })
		})
		.then(res => res.json())
		.then(response => {
		document.getElementById("villalift_submit_btn").innerHTML = "Submit";

		if (response?.data) {
			Swal.fire({
			icon: "success",
			text: "Thanks for submitting the details!",
			confirmButtonText: "OK"
			}).then(() => location.reload());
		} else {
			throw new Error("Unexpected response");
		}
		})
		.catch(err => {
		document.getElementById("villalift_submit_btn").innerHTML = "Submit";
		console.error(err);

		Swal.fire({
			icon: "error",
			text: "Failed to submit form"
		});
		});


	 });

});


