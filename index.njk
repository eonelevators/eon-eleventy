  <script>
	// ball and topline animations for Preferred_Lift_Speed starts
    const officelift_ball = document.getElementById("officelift_ball_Preferred_Lift_Speed");
  const officelift_topLine = document.getElementById("officelift_topLine_Preferred_Lift_Speed");
  const officelift_anim_container = document.getElementById("officelift_Preferred_Lift_Speed_anim_div");

  let officelift_speed = 0;
  let officelift_pos = 0;
  let officelift_direction = 1;
  let officelift_animating = false;

  const officelift_ballSize = 50;
  let officelift_floorPx = 0;
  let officelift_topPx = 0;

  function officelift_calculateBoundaries() {
    officelift_floorPx = 0;
    officelift_topPx = officelift_anim_container.offsetHeight+20 - (officelift_topLine.offsetTop + officelift_ballSize);
  }

  function officelift_animate() {
    if (!officelift_animating) return;

    officelift_pos += officelift_direction * (officelift_speed * 0.5);

    if (officelift_pos >= officelift_topPx) {
      officelift_pos = officelift_topPx;
      officelift_direction = -1;
    } else if (officelift_pos <= officelift_floorPx) {
      officelift_pos = officelift_floorPx;
      officelift_direction = 1;
    }

    officelift_ball.style.bottom = officelift_pos + "px";
    requestAnimationFrame(officelift_animate);
  }

  function officelift_setSpeed(newSpeed,obj) {
	//if(newSpeed==0){
	if(0){
		$("#officelift_ball_Preferred_Lift_Speed").css({"bottom":"0px"})
	}
	else{
		$(".officelift_Preferred_Lift_Speed_controls_btn").each(function(){
			if($(this).hasClass("btn-success")){
				$(this).removeClass("btn-success")
			}
			if($(this).hasClass("btn-primary")){
				$(this).removeClass("btn-primary")
			}
		});
		$(".officelift_Preferred_Lift_Speed_controls_btn").each(function(){
			$(this).addClass("btn-primary")
		});
		$(obj).removeClass("btn-primary")
		$(obj).addClass("btn-success")
		officelift_calculateBoundaries();
		officelift_speed = newSpeed;

		// reset to bottom when button is clicked
		officelift_pos = officelift_floorPx;
		officelift_ball.style.bottom = officelift_pos + "px";
		officelift_direction = 1;

		if (!officelift_animating) {
		  officelift_animating = true;
		  officelift_animate();
		}
	}
  }
  
 
