  <script>
	// ball and topline animations for Preferred_Lift_Speed starts
    const ball = document.getElementById("villalift_ball_Preferred_Lift_Speed");
  const topLine = document.getElementById("villalift_topLine_Preferred_Lift_Speed");
  const container = document.getElementById("villalift_Preferred_Lift_Speed_anim_div");

  let speed = 0;
  let pos = 0;
  let direction = 1;
  let animating = false;

  const ballSize = 50;
  let floorPx = 0;
  let topPx = 0;

  function calculateBoundaries() {
    floorPx = 0;
    topPx = container.offsetHeight - (topLine.offsetTop + ballSize);
  }

  function animate() {
    if (!animating) return;

    pos += direction * (speed * 0.5);

    if (pos >= topPx) {
      pos = topPx;
      direction = -1;
    } else if (pos <= floorPx) {
      pos = floorPx;
      direction = 1;
    }

    ball.style.bottom = pos + "px";
    requestAnimationFrame(animate);
  }

  function setSpeed(newSpeed,obj) {
	//if(newSpeed==0){
	if(0){
		$("#villalift_ball_Preferred_Lift_Speed").css({"bottom":"0px"})
	}
	else{
		$(".villalift_Preferred_Lift_Speed_controls_btn").each(function(){
			if($(this).hasClass("btn-success")){
				$(this).removeClass("btn-success")
			}
			if($(this).hasClass("btn-primary")){
				$(this).removeClass("btn-primary")
			}
		});
		$(".villalift_Preferred_Lift_Speed_controls_btn").each(function(){
			$(this).addClass("btn-primary")
		});
		$(obj).removeClass("btn-primary")
		$(obj).addClass("btn-success")
		calculateBoundaries();
		speed = newSpeed;

		// reset to bottom when button is clicked
		pos = floorPx;
		ball.style.bottom = pos + "px";
		direction = 1;

		if (!animating) {
		  animating = true;
		  animate();
		}
	}
  }
	// ball and topline animations for Preferred_Lift_Speed ends
  </script>
  
