  <script>
	// ball and topline animations for Preferred_Lift_Speed starts
    const hospitallift_ball = document.getElementById("hospitallift_ball_Preferred_Lift_Speed");
  const hospitallift_topLine = document.getElementById("hospitallift_topLine_Preferred_Lift_Speed");
  const hospitallift_anim_container = document.getElementById("hospitallift_Preferred_Lift_Speed_anim_div");

  let hospitallift_speed = 0;
  let hospitallift_pos = 0;
  let hospitallift_direction = 1;
  let hospitallift_animating = false;

  const hospitallift_ballSize = 50;
  let hospitallift_floorPx = 0;
  let hospitallift_topPx = 0;

  function hospitallift_calculateBoundaries() {
    hospitallift_floorPx = 0;
    hospitallift_topPx = hospitallift_anim_container.offsetHeight+20 - (hospitallift_topLine.offsetTop + hospitallift_ballSize);
  }

  function hospitallift_animate() {
    if (!hospitallift_animating) return;

    hospitallift_pos += hospitallift_direction * (hospitallift_speed * 0.5);

    if (hospitallift_pos >= hospitallift_topPx) {
      hospitallift_pos = hospitallift_topPx;
      hospitallift_direction = -1;
    } else if (hospitallift_pos <= hospitallift_floorPx) {
      hospitallift_pos = hospitallift_floorPx;
      hospitallift_direction = 1;
    }

    hospitallift_ball.style.bottom = hospitallift_pos + "px";
    requestAnimationFrame(hospitallift_animate);
  }

  function hospitallift_setSpeed(newSpeed,obj) {
	//if(newSpeed==0){
	if(0){
		$("#hospitallift_ball_Preferred_Lift_Speed").css({"bottom":"0px"})
	}
	else{
		$(".hospitallift_Preferred_Lift_Speed_controls_btn").each(function(){
			if($(this).hasClass("btn-success")){
				$(this).removeClass("btn-success")
			}
			if($(this).hasClass("btn-primary")){
				$(this).removeClass("btn-primary")
			}
		});
		$(".hospitallift_Preferred_Lift_Speed_controls_btn").each(function(){
			$(this).addClass("btn-primary")
		});
		$(obj).removeClass("btn-primary")
		$(obj).addClass("btn-success")
		hospitallift_calculateBoundaries();
		hospitallift_speed = newSpeed;

		// reset to bottom when button is clicked
		hospitallift_pos = hospitallift_floorPx;
		hospitallift_ball.style.bottom = hospitallift_pos + "px";
		hospitallift_direction = 1;

		if (!hospitallift_animating) {
		  hospitallift_animating = true;
		  hospitallift_animate();
		}
	}
  }
  
 
