  <script>
	// ball and topline animations for Preferred_Lift_Speed starts
    const apartmentlift_ball = document.getElementById("apartmentlift_ball_Preferred_Lift_Speed");
  const apartmentlift_topLine = document.getElementById("apartmentlift_topLine_Preferred_Lift_Speed");
  const apartmentlift_anim_container = document.getElementById("apartmentlift_Preferred_Lift_Speed_anim_div");

  let apartmentlift_speed = 0;
  let apartmentlift_pos = 0;
  let apartmentlift_direction = 1;
  let apartmentlift_animating = false;

  const apartmentlift_ballSize = 50;
  let apartmentlift_floorPx = 0;
  let apartmentlift_topPx = 0;

  function apartmentlift_calculateBoundaries() {
    apartmentlift_floorPx = 0;
    apartmentlift_topPx = apartmentlift_anim_container.offsetHeight+20 - (apartmentlift_topLine.offsetTop + apartmentlift_ballSize);
  }

  function apartmentlift_animate() {
    if (!apartmentlift_animating) return;

    apartmentlift_pos += apartmentlift_direction * (apartmentlift_speed * 0.5);

    if (apartmentlift_pos >= apartmentlift_topPx) {
      apartmentlift_pos = apartmentlift_topPx;
      apartmentlift_direction = -1;
    } else if (apartmentlift_pos <= apartmentlift_floorPx) {
      apartmentlift_pos = apartmentlift_floorPx;
      apartmentlift_direction = 1;
    }

    apartmentlift_ball.style.bottom = apartmentlift_pos + "px";
    requestAnimationFrame(apartmentlift_animate);
  }

  function apartmentlift_setSpeed(newSpeed,obj) {
	//if(newSpeed==0){
	if(0){
		$("#apartmentlift_ball_Preferred_Lift_Speed").css({"bottom":"0px"})
	}
	else{
		$(".apartmentlift_Preferred_Lift_Speed_controls_btn").each(function(){
			if($(this).hasClass("btn-success")){
				$(this).removeClass("btn-success")
			}
			if($(this).hasClass("btn-primary")){
				$(this).removeClass("btn-primary")
			}
		});
		$(".apartmentlift_Preferred_Lift_Speed_controls_btn").each(function(){
			$(this).addClass("btn-primary")
		});
		$(obj).removeClass("btn-primary")
		$(obj).addClass("btn-success")
		apartmentlift_calculateBoundaries();
		apartmentlift_speed = newSpeed;

		// reset to bottom when button is clicked
		apartmentlift_pos = apartmentlift_floorPx;
		apartmentlift_ball.style.bottom = apartmentlift_pos + "px";
		apartmentlift_direction = 1;

		if (!apartmentlift_animating) {
		  apartmentlift_animating = true;
		  apartmentlift_animate();
		}
	}
  }
  
 
	// apartmentlift_ball and apartmentlift_topLine animations for Preferred_Lift_Speed ends
 
 
  
  
 
