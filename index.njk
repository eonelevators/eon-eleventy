function getCheckedRadioValueFromParent_hospitallift(actual_radio_button_name) {

	if(actual_radio_button_name=="hospitallift_number_of_floors"){ 
		// this is special case since hospitallift_number_of_floors radio set is already under hidden div only (hospitallift_number_of_floors_div_inside is hidden) wantedly I did because of converting this to dropdown G+1 to G+30
		actual_div=actual_radio_button_name+"_div"
		actual_div_inside=actual_radio_button_name+"_div_inside"
			
		return $("#"+actual_div+"  ."+actual_div_inside+":hidden")
			.find('input[type="radio"][name="'+actual_radio_button_name+'"]:checked')
			.val() || "";
  }
  else{
	actual_div=actual_radio_button_name+"_div"
	actual_div_inside=actual_radio_button_name+"_div_inside"
	
	
	$("#" + actual_div + " ." + actual_div_inside + ":hidden")
		.find('input[type="radio"][name="' + actual_radio_button_name + '"]:checked')
		.prop('checked', false);  // if the hidden div radio buttons checked by force it will become false
		
		
	return $("#"+actual_div+"  ."+actual_div_inside+":visible")
		.find('input[type="radio"][name="'+actual_radio_button_name+'"]:checked')
		.val() || "";
  }
}
		
function getCheckedRadioCountFromParent_hospitallift(next_radio_button_name) {
  next_div=next_radio_button_name+"_div"
  next_div_inside=next_radio_button_name+"_div_inside"
  
  return $("#"+next_div+"  ."+next_div_inside+":visible")
    .find('input[type="radio"][name="'+next_radio_button_name+'"]:checked')
    .length;
}
