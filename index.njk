<script>
document.addEventListener("DOMContentLoaded", function () {

  function initTabs(containerEl, tabSelector, panelSelector) {
    const tabs = containerEl.querySelectorAll(tabSelector);
    const panels = containerEl.querySelectorAll(panelSelector);
    if (!tabs.length || !panels.length) return;

    // init: show first only
    panels.forEach((p, i) => p.style.display = i === 0 ? "block" : "none");
    tabs.forEach((t, i) => t.classList.toggle("active", i === 0));

    tabs.forEach(tab => {
      tab.addEventListener("click", function () {
        if (this.classList.contains("active")) return;

        tabs.forEach(t => t.classList.remove("active"));
        this.classList.add("active");

        panels.forEach(p => p.style.display = "none");

        const targetId = this.getAttribute("data-tab");
        const targetEl = document.getElementById(targetId);
        if (targetEl) targetEl.style.display = "block";
      });
    });
  }

  // MAIN TABS
  initTabs(document.querySelector(".get-quat-list-tab"), ".main-tabs li", ".tab_content > .tabs_item");

  // INSIDE PRODUCT TABS
  const nestContainer = document.getElementById("nest-products");
  if (nestContainer) initTabs(nestContainer, ".nest-products-tabs li", ".product-tab");

  const metroContainer = document.getElementById("metro-products");
  if (metroContainer) initTabs(metroContainer, ".metro-products-tabs li", ".product-tab");

  const summitContainer = document.getElementById("summit-products");
  if (summitContainer) initTabs(summitContainer, ".summit-products-tabs li", ".product-tab");

  // EXPLORE PRODUCTS (no go-back)
  document.querySelectorAll(".explore-btn").forEach(btn => {
    btn.addEventListener("click", function (e) {
      e.preventDefault();

      const targetId = this.dataset.target;
      const parentPanel = this.closest(".tabs_item");
      const targetPanel = document.getElementById(targetId);

      if (!parentPanel || !targetPanel) return;

      parentPanel.style.display = "none";
      targetPanel.style.display = "block";
    });
  });

});
</script>


