document.getElementById('officelift_floor_select')
  .addEventListener('change', function () {

    const val = this.value;
    if (!val) return;

    const radio = document.querySelector(
      `.officelift_floor-radio[value="${val}"]`
    );

    if (radio) {
      radio.checked = true;
      radio.dispatchEvent(new Event('change', { bubbles: true }));
    }
});

							
						



document.querySelectorAll(".officelift_floor-radio").forEach(radio => {
  radio.addEventListener("change", function() {
  
  
  
    // Hide all officelift_passengers-group
	document
	  .querySelectorAll(".officelift_passengers-group")
	  .forEach(g => (g.style.display = "none"));

	// Selected floor (ex: "G+3")
	const selected_floor_for_passengers = this.dataset.floor;

	// Find matching group where selected floor exists in data-openings
	document.querySelectorAll(".officelift_passengers-group").forEach(group => {
	  const floors_for_passengers = group.dataset.floor
		?.split(",")
